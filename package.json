{
  "name": "generic-token-oauth",
  "version": "0.2.0",
  "description": "Provides token oauth authorization without redirecting for any third-party service",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "token"
  ],
  "author": {
    "name": "Victor Krivonos",
    "email": "victor.krivonos.dev@gmail.com",
    "url": "https://github.com/viravir"
  },
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.2",
    "@types/oauth": "^0.9.1",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "mocha": "^7.1.0",
    "prettier": "^1.19.1",
    "sinon": "^9.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "oauth": "^0.9.15"
  }
}
